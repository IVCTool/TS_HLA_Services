<?xml version="1.0" encoding="UTF-8"?>
<schema targetNamespace="FCTT_NG" xmlns="http://www.w3.org/2001/XMLSchema" xmlns:tns="FCTT_NG">

	<element name="ConfigurationCertificationHLA" type="tns:ConfigurationCertificationHLA" />

	<complexType name="ConfigurationCertificationHLA">
		<sequence>
			<element name="CertificationName" type="string" />
			<element name="DirectorySavingCertification" type="string" />
			<element name="FederationName" type="string" />
			<element name="FOMFiles" type="tns:FileList" />
			<element name="RTIType" type="string" />
			<element name="RTIAddressIP" type="string" />
			<element name="RTIPort" type="string" />
			<element name="FederateToCertifyName" type="string" />
			<element name="FederateToCertifySOMFiles" type="tns:FileList" />
		</sequence>
	</complexType>
	
	<complexType name="FileList">
		<sequence minOccurs="0" maxOccurs="unbounded">
			 <element name="File" type="string"/>
		</sequence>
	</complexType>

</schema>